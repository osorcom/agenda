{% extends 'base.html.twig' %}

{% block title %}Contactos{% endblock %}

{% block main %}
<h1>Contactos</h1>
<table>
  {% for c in contactos %}
  <tr>
    <td>{{ c.id }}</td>
    <td>{{ c.nom }}</td>
    <td>{{ c.email }}</td>
    <td>{{ c.telf }}</td>
    <td><button class="delete" data-id="{{ c.id }}">X</button></td>
  </tr>
  {% endfor %}
</table>
<script src="/js/ajax.js"></script>
{% endblock %}
